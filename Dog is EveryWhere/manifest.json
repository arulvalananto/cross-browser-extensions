{
    "manifest_version": 2,
    "name": "Dog is EveryWhere",
    "version": "1.0.0",
    "description": "display dog image instead of image in a page",

    "web_accessible_resources": ["dogs/*.jpg"],

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
